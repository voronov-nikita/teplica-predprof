<DrawerClickableItem@MDNavigationDrawerItem>

    text_color: (0, 0, 0, 0)
    icon_color: (0, 0, 0, 0)
    ripple_color: (0, 0, 0, 0)
    selected_color: (0, 0, 0, 0)


<DrawerLabelItem@MDNavigationDrawerItem>
    text_color: (0, 0, 0, 0)
    icon_color: (0, 0, 0, 0)
    focus_behavior: True
    selected_color: (0, 0, 0, 0)


MDScreen:

    MDNavigationLayout:

        MDScreenManager:

            MDScreen:

                MDTopAppBar:
                    pos_hint: {"top": 1, "x":0}
                    size_hint: (1, .0)
                    md_bg_color: (0, 0 , 0, 0)
                    left_action_items: [["menu",lambda x: nav_drawer.set_state("open")]]
                    color: "#FFFFFF"

        MDNavigationDrawer:
            id: nav_drawer
            radius: (0, 0, 0 , 0)

            MDNavigationDrawerMenu:

                MDNavigationDrawerLabel:
                    text: "Settings"

                DrawerClickableItem:
                    icon: "pencil"
                    text: "EDIT"

                DrawerClickableItem:
                    icon: "lightbulb"
                    text: "THEMES"

                MDNavigationDrawerDivider:

                DrawerLabelItem:
                    icon: "information-outline"
                    text: "Label"
